{
  "name": "bitfumes/blogg",
  "description": "Blog package, Blog",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "sarthaksavvy",
      "email": "sarthak@bitfumes.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "bitfumes/laravel-likker": "^2.0",
    "graham-campbell/markdown": "^11.1",
    "bitfumes/visits": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0@dev",
    "orchestra/testbench": "^4.0",
    "orchestra/database": "^4.0",
    "illuminate/support": "^5.7|^5.8|^6.0",
    "fzaninotto/faker": "^1.9@dev"
  },
  "autoload": {
    "psr-4": {
      "Bitfumes\\Blogg\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bitfumes\\Blogg\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bitfumes\\Blogg\\BloggServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:sarthaksavvy/redis-visits.git"
    }
  ]
}
