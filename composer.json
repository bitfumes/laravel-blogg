{
    "name": "bitfumes/blogg",
    "description": "Blog package, Blog",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "sarthaksavvy",
        "email": "sarthak@bitfumes.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "spatie/laravel-medialibrary": "^7.0.0",
        "bitfumes/laravel-likker": "^1.0",
        "graham-campbell/markdown": "^10.2@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.7",
        "orchestra/database": "^3.7",
        "illuminate/support": "^5.7",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Bitfumes\\Blogg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitfumes\\Blogg\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bitfumes\\Blogg\\BloggServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
